<Window x:Class="WorldSkills.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WorldSkills"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel>
            <Image Source="Images\wsr-300.png" Width="150" HorizontalAlignment="Left"></Image>
            <Button Content="Назад" HorizontalAlignment="Right"></Button>
            <Button Content="Вывод отчета" x:Name="ReportOutputWorld" Click="ReportOutputWorld_Click"></Button>
        </StackPanel>
        <Frame></Frame>
    </Grid>
</Window>
