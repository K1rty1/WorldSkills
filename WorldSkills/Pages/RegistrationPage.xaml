<Page x:Class="WorldSkills.Pages.RegistrationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WorldSkills.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RegistrationPage">

    <Grid>
        <StackPanel>
            <TextBox Text="Введите логин" x:Name="LoginBox"></TextBox>
            <TextBox></TextBox>
            <TextBox Text="Введите пароль" x:Name="PasswordBox"></TextBox>
            <TextBox></TextBox>
            <TextBox Text="Повторите пароль" x:Name="AgainPasswordBox"></TextBox>
            <TextBox></TextBox>
            <Button Content="Создать" x:Name="CreateButton" Click="CreateButton_Click"></Button>
        </StackPanel>
    </Grid>
</Page>
